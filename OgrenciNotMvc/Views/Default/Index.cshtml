@using OgrenciNotMvc.Models.EntityFramework
@model List<TBLDERSLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>DERS ID</th>
            <th>DERS ADI</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ders in Model)
        {
            <tr>
                <td>@ders.DERSID</td>
                <td>@ders.DERSAD</td>
            </tr>
        }
    </tbody>
</table>

