@using OgrenciNotMvc.Models.EntityFramework
@model List<TBLOGRENCILER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <td>OGRENCI ID</td>
            <td>OGRENCI AD</td>
            <td>OGRENCI SOYAD</td>
            <td>CINSIYET</td>
            <td>KULUP</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var ogr in Model)
            {
                <tr>
                    <td>@ogr.OGRENCIID</td>
                    <td>@ogr.OGRAD</td>
                    <td>@ogr.OGRSOYAD</td>
                    <td>@ogr.OGRCINSIYET</td>
                    <td>@ogr.OGRKULUP</td>
                </tr>
            }
    </tbody>
</table>
<a href="/Ogrenci/YeniOgrenci" class="btn btn-primary">Yeni Öğrenci Kaydı</a>

